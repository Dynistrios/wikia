						"chineseTL": "",
						"chineseNote": "",
						"japanese": "戦いをもって平和をもたらす。これこそ私ができるたった一つの役目である",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
